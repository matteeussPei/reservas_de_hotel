      "outputs": [],
      "source": [
        "# filtrando apenas para quantidades de adultos, crian칞as e beb칡s diferentes de\n",
        "# zero, Entendendo que n칚o h치 como esse tipo de reserva.\n",
        "filtro = (df['babies'] == 0) & (df['children'] == 0) & (df['adults'] == 0)\n",
        "df = df[~filtro]\n"
      ]
