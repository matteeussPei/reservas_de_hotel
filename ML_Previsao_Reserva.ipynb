      "source": [
        "# preenchendo os valores faltantes com zero e convertendo a coluna para inteiros\n",
        "df['children'] = df['children'].fillna(0)\n",
        "df['children'] = df['children'].astype('int64', errors='raise')\n"
      ]
    },
