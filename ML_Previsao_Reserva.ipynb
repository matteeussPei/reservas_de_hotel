      "source": [
        "# Definindo os intervalos: dias 1-7 s칚o a primeira semana, 8-14 s칚o a segunda, etc.\n",
        "bins = [0, 7, 14, 21, 28, 32]  # Usando 32 para incluir o dia 31 no 칰ltimo intervalo\n",
        "\n",
        "# Labels para cada semana\n",
        "labels = [1, 2, 3, 4, 5]  # Quatro semanas, ent칚o quatro labels\n",
        "\n",
        "# Criando a nova coluna 'week_of_month' utilizando pd.cut()\n",
        "df['week_of_month'] = pd.cut(df['arrival_date_day_of_month'], bins=bins, labels=labels, right=True, include_lowest=True)\n",
        "\n",
