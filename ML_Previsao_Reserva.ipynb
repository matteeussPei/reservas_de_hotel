      "source": [
        "# apagaremos a coluna 'company' devido ter quase 100% dos valores nulos e por\n",
        "# ser o ID da empresa/entidade que fez a reserva/respons치vel pelo pagamento da\n",
        "# reserva a coluna 'agent' por se o ID da ag칡ncia de viagens que fez a reserva\n",
        "\n",
        "df.drop(columns = ['agent', 'company'], inplace=True)"
      ]
    },
