      "outputs": [],
      "source": [
        "# criaremos uma nova vari치vel chamada 'equal_room', verificando se o quarto\n",
        "# reservado 칠 o mesmo que o entregue. Se sim, 'equal_room' receber치 valor 1, se\n",
        "# n칚o, receber치 valor 0.\n",
        "df['equal_room'] = 0\n",
        "\n",
        "for i in range(df.shape[0]):\n",
        "  if (df.at[i,'reserved_room_type'] == df.at[i,'assigned_room_type']):\n",
        "    df.at[i, 'equal_room'] = 1\n",
        "  else:\n",
        "    df.at[i, 'equal_room'] = 0\n"
      ]
    },
